{
  "name": "myoncreatereactapp",
  "version": "1.0.0",
  "description": "A starter app",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "npm-run-all --parallel dev:*",
    "test:watch": "jest --env=jsdom --watchAll",
    "test-server": "nodemon --exec mocha __server_tests__",
    "start": "node dist/server.js",
    "dev:nodemon": "nodemon -w dist dist/server.js",
    "compile-dev": "webpack",
    "compile": "SET NODE_ENV=production && webpack -p",
    "dev:watch": "webpack-dev-server --hot --inline",
    "compile-server": "babel server --presets es2015-node4 --out-dir dist --source-maps",
    "dev:watch-server": "babel server --presets es2015-node4 --out-dir dist --source-maps --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinion31/MyOnCreateReactApp.git"
  },
  "author": "Chris Cantu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pinion31/MyOnCreateReactApp/issues"
  },
  "homepage": "https://github.com/pinion31/MyOnCreateReactApp#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "mongoose": "^4.13.6",
    "nodemon": "^1.12.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "^2.2.0",
    "sass-loader": "^6.0.6",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-sass": "^2.2.1"
  }
}
